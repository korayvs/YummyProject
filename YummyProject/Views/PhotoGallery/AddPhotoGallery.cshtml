@model YummyProject.Models.PhotoGallery
@{
    ViewBag.Title = "AddPhotoGallery";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yeni Fotoğraf Ekleme Sayfası</h2>
<br />

<form method="post">

    <label>Fotoğraf Url</label>
    @Html.TextBoxFor(x => x.ImageUrl, new { @class = "form-control" })
    <p class="text-danger"> @Html.ValidationMessageFor(x => x.ImageUrl) </p>
    <br />

    <button class="btn btn-outline-success">Kaydet</button>

</form>