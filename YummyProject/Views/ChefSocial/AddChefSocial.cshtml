@model YummyProject.Models.ChefSocial
@{
    ViewBag.Title = "AddChefSocial";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yeni Şef Ekleme Sayfası</h2>

<form method="post">

    @Html.HiddenFor(x => x.ChefId)
    @Html.HiddenFor(x => x.ChefSocialId)
    <label>Url</label>
    @Html.TextBoxFor(x => x.Url, new { @class = "form-control" })
    <p class="text-danger"> @Html.ValidationMessageFor(x => x.Url) </p>
    <br />
    <label>Icon </label>
    @Html.TextBoxFor(x => x.Icon, new { @class = "form-control" })
    <p class="text-danger"> @Html.ValidationMessageFor(x => x.Icon) </p>
    <br />
    <label>Name</label>
    @Html.TextBoxFor(x => x.SocialMediaName, new { @class = "form-control" })
    <p class="text-danger"> @Html.ValidationMessageFor(x => x.SocialMediaName) </p>
    <br />

    <button class="btn btn-outline-success">Submit</button>

</form>