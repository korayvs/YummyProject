@model YummyProject.Models.Category
@{
    ViewBag.Title = "AddCategory";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yeni Kategori Ekleme Sayfası</h2>
<br />

<form method="post">

    <label>Kategori Adı</label>
    @Html.TextBoxFor(x => x.CategoryName, new { @class = "form-control" })
    <p class="text-danger"> @Html.ValidationMessageFor(x => x.CategoryName) </p>
    <br />

    <button class="btn btn-outline-success">Kaydet</button>

</form>